<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.maciek.sleepwell">

    <uses-permission android:name="android.permission.RECORD_AUDIO" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity.Fragments.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity.MainActivity" />
        <activity android:name=".SleepingActivity.SleepingActivity" />
        <activity android:name=".AudioProcessing" />
        <activity android:name=".SleepingActivity.ProgressActivity"></activity>
    </application>

</manifest><!--
    android:theme="@style/AppTheme"
    android:theme="@style/Theme.AppCompat.Light.NoActionBar"
-->
